{
  "name": "summer-of-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen": "graphql-codegen --config codegen.yml --require dotenv/config",
    "start": "yarn gen && react-scripts start",
    "build": "yarn gen && react-scripts build"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.0",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/fragment-matcher": "^1.17.8",
    "@graphql-codegen/schema-ast": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "graphql": "^15.3.0",
    "graphql.macro": "^1.4.2",
    "prettier": "^2.1.2",
    "react-scripts": "^3.4.3",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": ["introspection-result.ts"]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
